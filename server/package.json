{
  "name": "affine-dashboard-api",
  "private": true,
  "version": "1.0.0",
  "description": "Affine Dashboard API Server - Express.js with PostgreSQL",
  "main": "index.cjs",
  "type": "commonjs",
  "scripts": {
    "start": "node index.cjs",
    "dev": "NODE_ENV=development node index.cjs",
    "pm2:start": "pm2 start ../ecosystem.config.js --cwd ..",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:monit": "pm2 monit",
    "pm2:logs": "pm2 logs",
    "test": "node test-db.cjs"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "pg-query-stream": "^4.10.3",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "node-fetch": "^3.3.2"
  },
  "keywords": [
    "affine",
    "dashboard",
    "api",
    "express",
    "postgresql",
    "pm2"
  ],
  "author": "Affine Dashboard Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}